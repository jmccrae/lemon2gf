SELECT ?blank ?frame ?subject ?directObject ?indirectObject ?prepositionalObject ?synArg WHERE { #slurp 
## 
 <\$uri> <http://www.monnet-project.eu/lemon#synBehavior> ?blank .
 ?blank a ?frame . #slurp
##   
 OPTIONAL { #slurp
 #set $sep = ''
 #for $p in $subject
 $sep { ?blank <$p> ?subject . } #slurp
 #set $sep = ' UNION ' 
 #end for
 } #slurp
##
 OPTIONAL { #slurp
 #set $sep = ''
 #for $p in $directObject
 $sep { ?blank <$p> ?directObject . } #slurp
 #set $sep = ' UNION ' 
 #end for
 } #slurp
##
 OPTIONAL { #slurp
 #set $sep = ''
 #for $p in $indirectObject
 $sep { ?blank <$p> ?indirectObject . } #slurp
 #set $sep = ' UNION ' 
 #end for
 } #slurp
##
 OPTIONAL { #slurp
 #set $sep = ''
 #for $p in $prepositionalObject
 $sep { ?blank <$p> ?prepositionalObject . } #slurp
 #set $sep = ' UNION ' 
 #end for
 } #slurp
 OPTIONAL { ?blank <http://www.monnet-project.eu/lemon#synArg> ?synArg . } #slurp
##
}
